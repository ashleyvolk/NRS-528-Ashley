import csv

with open('Species_Data.csv') as csv_file:
    csv_reader = csv.reader(csv_file, delimiter=',')

Species_Data = []
for row in csv_reader: #This row is giving me an I/O error
    if row[0] not in Species_Data:
        Species_Data.append(row[0])

